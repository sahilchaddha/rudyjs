  "bugs": {
    "url": "https://github.com/sahilchaddha/rudy/issues"
  },
  "homepage": "https://github.com/sahilchaddha/rudy#readme",
  "dependencies": {
    "chalk": "^2.4.1",
    "commander": "^2.15.1",
    "request": "^2.85.0"
  },
  "devDependencies": {
    "@types/node": "^10.0.4",
    "@types/request": "^2.47.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "mocha": "^5.1.1",
